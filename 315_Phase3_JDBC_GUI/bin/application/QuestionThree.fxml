<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.QuestionThreeController">
   <children>
      <Pane maxHeight="200.0" maxWidth="400.0" minHeight="200.0" minWidth="400.0" prefHeight="200.0" prefWidth="400.0">
         <children>
            <VBox maxHeight="150.0" maxWidth="400.0" minHeight="150.0" minWidth="400.0" prefHeight="150.0" prefWidth="400.0">
               <children>
                  <Label text="Given a team, is there a correlation between average attendance ">
                     <VBox.margin>
                        <Insets left="25.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="and team record.">
                     <VBox.margin>
                        <Insets left="25.0" />
                     </VBox.margin>
                  </Label>
                  <HBox maxHeight="50.0" maxWidth="400.0" minHeight="50.0" minWidth="400.0" prefHeight="50.0" prefWidth="400.0">
                     <children>
                        <Label text="Team:">
                           <HBox.margin>
                              <Insets left="25.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="TeamNameText">
                           <HBox.margin>
                              <Insets left="15.0" top="10.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox maxHeight="50.0" maxWidth="400.0" minHeight="50.0" minWidth="400.0" prefHeight="50.0" prefWidth="400.0">
                     <children>
                        <Label text="Year:">
                           <HBox.margin>
                              <Insets left="25.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <MenuButton fx:id="yearButton" mnemonicParsing="false" text="MenuButton">
                          <items>
                            <MenuItem fx:id="item2005" mnemonicParsing="false" onAction="#selected2005" text="2005" />
                            <MenuItem fx:id="item2006" mnemonicParsing="false" onAction="#selected2006" text="2006" />
                              <MenuItem fx:id="item2007" mnemonicParsing="false" onAction="#selected2007" text="2007" />
                              <MenuItem fx:id="item2008" mnemonicParsing="false" onAction="#selected2008" text="2008" />
                              <MenuItem fx:id="item2009" mnemonicParsing="false" onAction="#selected2009" text="2009" />
                              <MenuItem fx:id="item2010" mnemonicParsing="false" onAction="#selected2010" text="2010" />
                              <MenuItem fx:id="item2011" mnemonicParsing="false" onAction="#selected2011" text="2011" />
                              <MenuItem fx:id="item2012" mnemonicParsing="false" onAction="#selected2012" text="2012" />
                              <MenuItem fx:id="item2013" mnemonicParsing="false" onAction="#selected2013" text="2013" />
                          </items>
                           <HBox.margin>
                              <Insets left="23.0" top="10.0" />
                           </HBox.margin>
                        </MenuButton>
                     </children>
                  </HBox>
                  <Button fx:id="OKButton" mnemonicParsing="false" onAction="#OKPressed" text="OK">
                     <VBox.margin>
                        <Insets left="25.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
